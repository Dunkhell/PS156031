#include <stdio.h>
#include <stdlib.h>
void foo(unsigned int n,unsigned int m, unsigned int k)
{
    for (int i=2*n+1;i<=3*m;i++)
    {
        if (i%k==0)
        {
            printf("Liczba: %i\n",i);
        }
    }
}
int main()
{
    int n=1,k=2,m=10;
    foo(n,m,k);
    printf("przypadek 2:\n");
    int x=2,y=3,z=1;
    foo(x,y,z);
    return 0;
}
