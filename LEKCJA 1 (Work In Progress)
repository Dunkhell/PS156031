#include <stdio.h>
#include <stdlib.h>

int main()
{
    int liczba=0;
    scanf("%i", &liczba);
    printf("Twoja liczba to %i", liczba);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float liczba=0;
    scanf("%f", &liczba);
    printf("Twoja liczba to %f", liczba);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int liczba1=0, liczba2=0, liczba3=0;
    scanf("%i %i %i", &liczba1, &liczba2, &liczba3);
    printf("%i\n%i\n%i",liczba1,liczba2,liczba3);
return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int liczba=0;
    scanf("%i", &liczba);
    printf("Licba o 1 wieksza od %i to %i", liczba, liczba+1);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int liczba,liczba2,liczba3;
    scanf("%i %i %i",&liczba,&liczba2,&liczba3);
    printf("%f",(float)(liczba+liczba2+liczba3)/3);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    float x;
    do
    {
        printf("Liczba musi byc nieujemna\n");
        scanf("%f",&x);
    }
    while(x<0);
    printf("Pierwiastek %f wynosi %f",x, sqrt(x));
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float x;
    scanf("%f",&x);

    printf("Wartosc bezwzgledna z %f to %f\n", x, fabs(x) );
    return 0;
}
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float x;
    scanf("%f", &x);
    printf("%.2f",x);
    return 0;
}
#include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main()
{
    float x;
    scanf("%f", &x);
    printf("%.1e",x);
    return 0;
}
