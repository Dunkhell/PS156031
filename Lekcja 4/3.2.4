#include <stdio.h>
#include <stdlib.h>
void foo (int *a, int *b)
{
    int x= *a;
    if (*b < *a)
    {
        *a = *b;
        *b = x;
    }
}
int main()
{
    int a,b;
    scanf("%i",&a);
    scanf("%i",&b);
    foo(&a,&b);
    printf("Wartosc a wynosi %i | Wartosc b wynosi %i",a,b);
    return 0;
}
