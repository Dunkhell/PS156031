#include <stdio.h>
#include <stdlib.h>
int potega(int a, int b)
{
    int wynik=1;
    for (int i=1; i <= b; i++)
    {
        wynik=wynik*a;
    }
    return  wynik;
}
int pierw (int n, int m)
{
    int wynik=0;
    for (int i=1; potega(i,m)<=n;i++)
    {
        wynik=i;
    }
    return wynik;
}
int main()
{
    int n,m;
    do
    {
        scanf("%i",&n);
    }while (n<0);
   do
    {
        scanf("%i",&m);
    }while (n<1);
    printf("Pierwiastek o podstawie %i z liczby %i wynosi %i",m,n,pierw(n,m));
    return 0;
}
