#include <stdio.h>
#include <stdlib.h>
void wytnij (char*napis, int n, int m)
{
    int i,dl;
    for (dl=0;napis[dl]!=0;dl++);
    if (dl+1>m)
    {
        for (i=0;i+m<dl;i++)
        {
            napis[n+i]=napis[i+m+1];
        }
    }
    else
        if ((n<dl)&&(dl+1<=m))
            napis[n]=0;
}

int main()
{
    char napis[]="1234wyciac789";
    wytnij(napis,1,2);
    printf("%s\n",napis);
    return 0;
}
