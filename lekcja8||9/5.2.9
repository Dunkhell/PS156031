#include <stdio.h>
#include <stdlib.h>
void wytnij (char* napis, int n, int m)
{
    int i,l;
    for (l=0; napis[l]!=0;l++)
    {
        if (l +1>m)
        {
            for (i=0; i+m<l;i++)
            {
                napis[i+n]= napis[i+m+1];
            }
        }
        else
        {
            if ((n<l) && (l+1<=m))
            {
                napis[n]=0;
            }
        }
    }

}
int main()
{
    char napis[]="1234wyciac789";
    wytnij(napis,4,6);
    printf("%s\n",napis);
    return 0;
}
